/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP One2Two {
    IN a, b;
    OUT out[2];

    PARTS:
    // Put your code here:
    Pass(in=a, out=out[0]);
    Pass(in=b, out=out[1]);
}
